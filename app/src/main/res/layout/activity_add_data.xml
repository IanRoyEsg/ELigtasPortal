<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#bfdbfe"
    tools:context=".Add_data">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="368dp"
        android:layout_height="94dp"
        android:layout_marginStart="21dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="22dp"
        android:layout_marginBottom="609dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/eligtasimage" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="156dp"
        android:layout_marginTop="583dp"
        android:layout_marginEnd="155dp"
        android:layout_marginBottom="100dp"
        android:background="@drawable/bg_rounded3"
        android:onClick="submit"
        android:padding="20px"
        android:text="Add Data"
        android:textColor="@color/white"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="162dp"
        android:layout_marginTop="658dp"
        android:layout_marginEnd="161dp"
        android:layout_marginBottom="25dp"
        android:background="@drawable/bg_rounded3"
        android:padding="20px"
        android:text="back"
        android:textColor="@color/white"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="138dp"
        android:layout_marginEnd="352dp"
        android:layout_marginBottom="574dp"
        android:text="Title"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/etTitle"
        android:layout_width="358dp"
        android:layout_height="47dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="166dp"
        android:layout_marginEnd="23dp"
        android:layout_marginBottom="518dp"
        android:background="@drawable/bg_rounded2"
        android:ems="10"
        android:hint="Title"
        android:inputType="textPersonName"
        android:padding="20px"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="223dp"
        android:layout_marginEnd="330dp"
        android:layout_marginBottom="489dp"
        android:text="Content"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/etContent"
        android:layout_width="355dp"
        android:layout_height="171dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="250dp"
        android:layout_marginEnd="22dp"
        android:layout_marginBottom="310dp"
        android:background="@drawable/bg_rounded2"
        android:ems="10"
        android:gravity="top"
        android:hint="Content"
        android:inputType="textMultiLine"
        android:padding="20px"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/etUserid"
        android:layout_width="341dp"
        android:layout_height="60dp"
        android:layout_marginStart="42dp"
        android:layout_marginTop="463dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="208dp"
        android:background="@drawable/bg_rounded2"
        android:ems="10"
        android:hint="User ID"
        android:inputType="textPersonName"
        android:padding="20px"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="34dp"
        android:layout_marginTop="430dp"
        android:layout_marginEnd="331dp"
        android:layout_marginBottom="282dp"
        android:text="User ID"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>